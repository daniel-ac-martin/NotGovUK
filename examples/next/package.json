{
  "name": "@not-govuk/next-example",
  "version": "0.9.1",
  "description": "A Next.js application with NotGovUK components.",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npm run test:unit",
    "test:ci": "npm test && npm run test:functional:ci",
    "test:functional": "cypress run ${CYPRESS_PROJECT_ID:+--record ${CYPRESS_FLAGS}}",
    "test:functional:ci": "start-server-and-test 'start' 'http-get://localhost:8080/' test:functional",
    "test:unit": "jest --passWithNoTests"
  },
  "dependencies": {
    "@not-govuk/simple-components": "workspace:^0.9.1",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cypress": "13.13.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "start-server-and-test": "2.0.5",
    "ts-jest": "29.2.3",
    "typescript": "^5"
  }
}
