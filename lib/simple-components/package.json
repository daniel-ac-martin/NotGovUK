{
  "name": "@not-govuk/simple-components",
  "version": "0.17.1",
  "description": "Simple NotGovUK components.",
  "main": "src/index.ts",
  "sass": "assets/index.scss",
  "exports": {
    ".": {
      "sass": "./assets/index.scss",
      "default": "./src/index.ts"
    }
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "typings": "dist/index.d.ts",
    "exports": {
      ".": {
        "sass": "./assets/index.scss",
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "/assets",
    "/dist"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "prepublishOnly": "npm run clean && npm run build",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -m es2022 && find dist -name '*.js' -exec sh -c 'mv \"$0\" \"${0%.js}.mjs\"' {} \\;",
    "build:cjs": "tsc",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "author": "Daniel A.C. Martin <npm@daniel-martin.co.uk> (http://daniel-martin.co.uk/)",
  "license": "MIT",
  "keywords": [
    "react-components"
  ],
  "dependencies": {
    "@not-govuk/aside": "workspace:^",
    "@not-govuk/back-link": "workspace:^",
    "@not-govuk/breadcrumbs": "workspace:^",
    "@not-govuk/button": "workspace:^",
    "@not-govuk/checkboxes": "workspace:^",
    "@not-govuk/cookie-banner": "workspace:^",
    "@not-govuk/date-input": "workspace:^",
    "@not-govuk/details": "workspace:^",
    "@not-govuk/error-message": "workspace:^",
    "@not-govuk/error-summary": "workspace:^",
    "@not-govuk/fieldset": "workspace:^",
    "@not-govuk/file-upload": "workspace:^",
    "@not-govuk/footer": "workspace:^",
    "@not-govuk/form-field": "workspace:^",
    "@not-govuk/form-group": "workspace:^",
    "@not-govuk/header": "workspace:^",
    "@not-govuk/hint": "workspace:^",
    "@not-govuk/input": "workspace:^",
    "@not-govuk/inset-text": "workspace:^",
    "@not-govuk/label": "workspace:^",
    "@not-govuk/link": "workspace:^",
    "@not-govuk/navigation-menu": "workspace:^",
    "@not-govuk/notification-banner": "workspace:^",
    "@not-govuk/page": "workspace:^",
    "@not-govuk/pagination": "workspace:^",
    "@not-govuk/panel": "workspace:^",
    "@not-govuk/phase-banner": "workspace:^",
    "@not-govuk/radios": "workspace:^",
    "@not-govuk/sass-base": "workspace:^",
    "@not-govuk/search-box": "workspace:^",
    "@not-govuk/select": "workspace:^",
    "@not-govuk/service-navigation": "workspace:^",
    "@not-govuk/skip-link": "workspace:^",
    "@not-govuk/standalone-input": "workspace:^",
    "@not-govuk/summary-card": "workspace:^",
    "@not-govuk/summary-list": "workspace:^",
    "@not-govuk/table": "workspace:^",
    "@not-govuk/tabs": "workspace:^",
    "@not-govuk/tag": "workspace:^",
    "@not-govuk/text-input": "workspace:^",
    "@not-govuk/textarea": "workspace:^",
    "@not-govuk/warning-text": "workspace:^"
  },
  "devDependencies": {
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  }
}
