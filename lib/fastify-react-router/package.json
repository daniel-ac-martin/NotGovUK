{
  "name": "@not-govuk/fastify-react-router",
  "version": "0.16.3",
  "description": "Fastify plugin for React Router.",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./dev": "./src/dev.ts"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "typings": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "default": "./dist/index.mjs"
      },
      "./dev": {
        "types": "./dist/dev.d.ts",
        "import": "./dist/dev.mjs",
        "require": "./dist/dev.js",
        "default": "./dist/dev.mjs"
      }
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "prepublishOnly": "npm run clean && npm run build",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -m es2022 && find dist -name '*.js' -exec sh -c 'mv \"$0\" \"${0%.js}.mjs\"' {} \\;",
    "build:cjs": "tsc",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "author": "Daniel A.C. Martin <npm@daniel-martin.co.uk> (http://daniel-martin.co.uk/)",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@fastify/middie": "^9.1.0",
    "@fastify/static": "^9.0.0",
    "@not-govuk/react-router-context": "workspace:^",
    "@react-router/node": "^7.12.0",
    "fastify-plugin": "^5.1.0"
  },
  "devDependencies": {
    "@not-govuk/fastify-auth": "workspace:*",
    "@not-govuk/fastify-harden": "workspace:*",
    "fastify": "5.7.1",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "react-router": "7.12.0",
    "rollup": "4.55.1",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  },
  "peerDependencies": {
    "react-router": "^7.12.0",
    "vite": "^7.3.1"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  }
}
