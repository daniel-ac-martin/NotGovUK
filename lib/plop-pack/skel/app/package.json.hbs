{
  "name": "@{{{pkg 'name'}}}/{{{name}}}",
  "version": "{{{pkg 'version'}}}",
  "description": "{{{description}}}",
  "private": true,
  "type": "module",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node .",
    "start:test": "COOKIES_SECURE=no node .",
    "test": "npm run test:unit",
    "test:ci": "npm test && npm run test:functional:ci",
    "test:functional": "cypress run ${CYPRESS_PROJECT_ID:+--record ${CYPRESS_FLAGS}}",
    "test:functional:ci": "start-server-and-test 'start:test' 'http-get://localhost:8080/readiness' test:functional",
    "test:unit": "jest --passWithNoTests",
    "build": "npm run build:app && npm run build:server",
    "build:app": "react-router build",
    "build:server": "vite -c vite.config.server.ts build",
    "heroku-postbuild": "npm run build",
    "clean": "rm -rf dist/ pkg/ node_modules/.vite/ node_modules/.vite-temp/ .react-router/",
    "dev": "NODE_ENV=development bun --watch --inspect ./src/server/dev.ts",
    "cypress": "cypress open",
    "create": "plop",
    "create:deployment": "plop deployment",
    "create:page": "plop page",
    "typecheck": "react-router typegen && tsc"
  },
  "author": "{{{pkg 'author'}}}",
  "license": "{{{pkg 'license'}}}",
  "dependencies": {
    "@mdx-js/rollup": "^3.1.1",
    "@not-govuk/fastify": "{{#if (eq (pkg 'name')'not-govuk')}}workspace:*{{else}}0.16.3{{/if}}",
    "@not-govuk/fastify-react-router": "{{#if (eq (pkg 'name')'not-govuk')}}workspace:*{{else}}0.16.3{{/if}}",
    "@not-govuk/react-router-context": "{{#if (eq (pkg 'name')'not-govuk')}}workspace:*{{else}}0.16.3{{/if}}",
    "@not-govuk/router": "{{#if (eq (pkg 'name')'not-govuk')}}workspace:*{{else}}0.16.3{{/if}}",
    "@react-router/fs-routes": "^7.9.5",
    "@react-router/node": "^7.9.5",
    "isbot": "^5.1.32",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "^7.9.5",
    "serverless-http": "^4.0.0"
  },
  "devDependencies": {
    "@not-govuk/app-plop-pack": "{{#if (eq (pkg 'name')'not-govuk')}}workspace:*{{else}}0.16.3{{/if}}",
    "@not-govuk/vite-html-react": "{{#if (eq (pkg 'name')'not-govuk')}}workspace:*{{else}}0.16.3{{/if}}",
    "@react-router/dev": "^7.9.5",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "bun": "^1.3.1",
    "cypress": "15.6.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "start-server-and-test": "2.1.2",
    "ts-jest": "29.4.5",
    "typescript": "5.9.3",
    "vite": "^7.2.2"
  }
}
