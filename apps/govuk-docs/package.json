{
  "name": "@not-govuk/govuk-docs",
  "version": "0.16.3",
  "description": "NotGovUK documentation website.",
  "private": true,
  "type": "module",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node .",
    "start:test": "COOKIES_SECURE=no node .",
    "test": "npm run test:unit",
    "test:ci": "npm test && npm run test:functional:ci",
    "test:functional": "cypress run ${CYPRESS_PROJECT_ID:+--record ${CYPRESS_FLAGS}}",
    "test:functional:ci": "start-server-and-test 'start:test' 'http-get://localhost:8080/readiness' test:functional",
    "test:unit": "jest --passWithNoTests",
    "build": "npm run build:app && npm run build:server",
    "build:app": "react-router build",
    "build:server": "vite -c vite.config.server.ts build",
    "clean": "rm -rf dist/ pkg/ node_modules/.vite/ node_modules/.vite-temp/ .react-router/",
    "dev": "NODE_ENV=development bun --watch ./src/server/dev.ts",
    "cypress": "cypress open",
    "typecheck": "react-router typegen && tsc"
  },
  "author": "Daniel A.C. Martin <npm@daniel-martin.co.uk> (http://daniel-martin.co.uk/)",
  "license": "MIT",
  "dependencies": {
    "@joshwooding/vite-plugin-react-docgen-typescript": "^0.6.2",
    "@mdx-js/rollup": "^3.1.1",
    "@not-govuk/anchor-list": "workspace:^0.16.3",
    "@not-govuk/components": "workspace:^0.16.3",
    "@not-govuk/docs-components": "workspace:^0.16.3",
    "@not-govuk/fastify": "workspace:^0.16.3",
    "@not-govuk/fastify-react-router": "workspace:^0.16.3",
    "@not-govuk/router": "workspace:^0.16.3",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "isbot": "^5.1.27",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "^7.9.5",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.7.1",
    "@storybook/csf-plugin": "^9.1.10",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "bun": "^1.2.23",
    "cypress": "14.5.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "start-server-and-test": "2.0.13",
    "ts-jest": "29.4.1",
    "typescript": "5.9.3",
    "vite": "^6.3.3"
  }
}
