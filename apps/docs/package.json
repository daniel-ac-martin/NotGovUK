{
  "name": "@not-govuk/docs",
  "version": "0.17.3",
  "description": "NotGovUK documentation website.",
  "private": true,
  "type": "module",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node .",
    "start:test": "COOKIES_SECURE=no node .",
    "test": "npm run test:unit",
    "test:ci": "npm test && npm run test:functional:ci",
    "test:functional": "cypress run ${CYPRESS_PROJECT_ID:+--record ${CYPRESS_FLAGS}}",
    "test:functional:ci": "start-server-and-test 'start:test' 'http-get://localhost:8080/readiness' test:functional",
    "test:unit": "jest --passWithNoTests",
    "build": "npm run build:app && npm run build:server",
    "build:app": "react-router build",
    "build:server": "vite -c vite.config.server.ts build",
    "heroku-postbuild": "npm run build",
    "clean": "rm -rf dist/ pkg/ node_modules/.vite/ node_modules/.vite-temp/ .react-router/",
    "dev": "NODE_ENV=development bun --watch --inspect ./src/server/dev.ts",
    "cypress": "cypress open",
    "create": "plop",
    "create:deployment": "plop deployment",
    "create:page": "plop page",
    "typecheck": "react-router typegen && tsc"
  },
  "author": "Daniel A.C. Martin <npm@daniel-martin.co.uk> (http://daniel-martin.co.uk/)",
  "license": "MIT",
  "dependencies": {
    "@joshwooding/vite-plugin-react-docgen-typescript": "0.6.4",
    "@mdx-js/rollup": "3.1.1",
    "@not-govuk/components": "workspace:*",
    "@react-foundry/anchor-list": "0.1.7",
    "@react-foundry/fastify": "0.1.7",
    "@react-foundry/fastify-react-router": "0.1.7",
    "@react-foundry/react-router-context": "0.1.7",
    "@react-foundry/router": "0.1.7",
    "@react-router/fs-routes": "7.13.0",
    "@react-router/node": "7.13.0",
    "isbot": "5.1.34",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router": "7.13.0",
    "serverless-http": "4.0.0"
  },
  "peerDependencies": {
    "@react-foundry/docs-components": "^0.1.7"
  },
  "devDependencies": {
    "@react-foundry/app-plop-pack": "0.1.7",
    "@react-foundry/vite-html-react": "0.1.7",
    "@react-router/dev": "7.13.0",
    "@storybook/csf-plugin": "9.1.17",
    "@types/mdx": "2.0.13",
    "@types/node": "24.10.13",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "bun": "1.3.9",
    "cypress": "15.10.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "sass-embedded": "1.97.3",
    "start-server-and-test": "2.1.3",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  }
}
