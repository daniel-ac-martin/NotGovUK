{
  "name": "@not-govuk/govuk-template",
  "version": "0.16.1",
  "description": "A starting point for writing a new NotGovUK application.",
  "private": true,
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node .",
    "start:test": "COOKIES_SECURE=no node .",
    "test": "npm run test:unit",
    "test:ci": "npm test && npm run test:functional:ci",
    "test:functional": "cypress run ${CYPRESS_PROJECT_ID:+--record ${CYPRESS_FLAGS}}",
    "test:functional:ci": "start-server-and-test 'start:test' 'http-get://localhost:8080/readiness' test:functional",
    "test:unit": "jest --passWithNoTests",
    "build": "webpack",
    "package:aws-lambda": "sls package -c aws.serverless.yml -p pkg/aws-lambda",
    "heroku-postbuild": "npm run build",
    "clean": "rm -rf .serverless dist pkg/aws-lambda",
    "dev": "NODE_ENV=development node-hot --fork | bunyan",
    "dev:ssr": "NODE_ENV=development SSR_ONLY=true node-hot --fork | bunyan",
    "cypress": "cypress open",
    "create": "plop",
    "create:deployment": "plop deployment",
    "create:page": "plop page"
  },
  "author": "Daniel A.C. Martin <npm@daniel-martin.co.uk> (http://daniel-martin.co.uk/)",
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "os": [
      "darwin",
      "linux"
    ]
  },
  "devDependencies": {
    "@apollo/client": "3.13.8",
    "@graphql-tools/schema": "10.0.23",
    "@not-govuk/anchor-list": "workspace:^0.16.1",
    "@not-govuk/app-composer": "workspace:^0.16.1",
    "@not-govuk/app-plop-pack": "workspace:^0.16.1",
    "@not-govuk/asset-proxy": "workspace:^0.16.1",
    "@not-govuk/client-component-helpers": "workspace:^0.16.1",
    "@not-govuk/client-renderer": "workspace:^0.16.1",
    "@not-govuk/components": "workspace:^0.16.1",
    "@not-govuk/engine": "workspace:^0.16.1",
    "@not-govuk/router": "workspace:^0.16.1",
    "@not-govuk/server-renderer": "workspace:^0.16.1",
    "@not-govuk/user-info": "workspace:^0.16.1",
    "@not-govuk/webpack-config": "workspace:^0.16.1",
    "@types/react": "18.3.22",
    "@types/react-dom": "18.3.7",
    "@types/webpack-env": "1.18.8",
    "bunyan": "1.8.15",
    "cypress": "14.4.0",
    "graphql": "15.10.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "node-hot-loader": "1.23.0",
    "plop": "4.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet-async": "2.0.5",
    "react-router": "6.30.1",
    "react-router-dom": "6.30.1",
    "serverless": "3.40.0",
    "start-server-and-test": "2.0.12",
    "ts-jest": "29.3.4",
    "typescript": "4.9.5",
    "webpack": "5.99.9",
    "webpack-cli": "5.1.4",
    "webpack-dev-middleware": "6.1.3",
    "webpack-hot-middleware": "2.26.1"
  }
}
