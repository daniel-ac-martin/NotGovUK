{
  "name": "@not-govuk/storybook",
  "version": "0.17.3",
  "description": "Storybook for NotGovUK.",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "serve -l 6006 dist",
    "build": "storybook build -c './src' -o './dist' --stats-json",
    "clean": "rm -rf dist",
    "dev": "storybook dev -c './src' -p 6006",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "author": "Daniel A.C. Martin <npm@daniel-martin.co.uk> (http://daniel-martin.co.uk/)",
  "license": "MIT",
  "dependencies": {
    "@not-govuk/components": "workspace:*",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "serve": "14.2.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.3",
    "@storybook/addon-a11y": "9.1.17",
    "@storybook/addon-docs": "9.1.17",
    "@storybook/react": "9.1.17",
    "@storybook/react-vite": "9.1.17",
    "@types/node": "24.10.13",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "react-router": "7.13.0",
    "sass-embedded": "1.97.3",
    "storybook": "9.1.17",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  }
}
