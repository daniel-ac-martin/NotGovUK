{
  "name": "not-govuk",
  "version": "0.1.0",
  "description": "A framework for quickly building digital services for government.",
  "scripts": {
    "create": "plop",
    "create:app": "plop app",
    "create:component": "plop component",
    "create:lib": "plop lib",
    "build": "pnpm recursive run build",
    "clean": "pnpm recursive exec -- rm -rf node_modules; rm shrinkwrap.yaml; rm -rf node_modules",
    "start": "pnpm recursive run start --filter @not-govuk/components",
    "storybook": "pnpm recursive run storybook --filter @not-govuk/components",
    "test": "jest --collectCoverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniel-ac-martin/NotGovUK.git"
  },
  "keywords": [
    "react-components"
  ],
  "author": "Daniel A.C. Martin <npm@daniel-martin.co.uk> (http://daniel-martin.co.uk/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daniel-ac-martin/NotGovUK/issues"
  },
  "homepage": "https://github.com/daniel-ac-martin/NotGovUK#readme",
  "devDependencies": {
    "@mdx-js/react": "^1.6.5",
    "@not-govuk/docs-components": "workspace:^0.1.0",
    "@storybook/addon-docs": "^6.0.0-rc.3",
    "@types/jest": "^26.0.3",
    "fs-extra": "^9.0.0",
    "jest": "^26.1.0",
    "plop": "^2.7.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
