{
  "name": "@not-govuk/components",
  "version": "0.14.0",
  "description": "React components implementing the GOV.UK Design System.",
  "main": "src/index.ts",
  "sass": "src/index.scss",
  "publishConfig": {
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  },
  "files": [
    "assets/",
    "dist/",
    "src/"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "build": "tsc",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniel-ac-martin/NotGovUK.git"
  },
  "keywords": [
    "react-components"
  ],
  "author": "Daniel A.C. Martin <npm@daniel-martin.co.uk> (http://daniel-martin.co.uk/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daniel-ac-martin/NotGovUK/issues"
  },
  "homepage": "https://github.com/daniel-ac-martin/NotGovUK#readme",
  "dependencies": {
    "@not-govuk/form": "workspace:^0.14.0",
    "@not-govuk/forms": "workspace:^0.14.0",
    "@not-govuk/sass-base": "workspace:^0.14.0",
    "@not-govuk/simple-components": "workspace:^0.14.0"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@not-govuk/component-test-helpers": "workspace:^0.14.0",
    "@types/node": "20.16.5",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "govuk-frontend": "5.6.0",
    "typescript": "4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@storybook)/)"
    ]
  }
}
