{
  "name": "@not-govuk/app-template",
  "version": "0.1.0",
  "description": "A starting point for writing a new NotGovUK application.",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:assets && npm run build:server",
    "build:assets": "webpack",
    "build:server": "webpack --config webpack.config.server.js",
    "clean": "rm -rf dist",
    "dev": "NODE_ENV=development node-hot --config webpack.config.server.js | bunyan",
    "dev:ssr": "NODE_ENV=development SSR_ONLY=true node-hot --config webpack.config.server.js | bunyan"
  },
  "author": "Daniel A.C. Martin <npm@daniel-martin.co.uk> (http://daniel-martin.co.uk/)",
  "license": "ISC",
  "dependencies": {
    "@not-govuk/components": "workspace:^0.1.0",
    "@not-govuk/engine": "workspace:^0.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "webpack": "^4.43.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@not-govuk/app-composer": "workspace:^0.1.0",
    "@not-govuk/client-renderer": "workspace:^0.1.0",
    "@not-govuk/server-renderer": "workspace:^0.1.0",
    "@not-govuk/webpack-config": "workspace:^0.1.0",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "bunyan": "^1.8.12",
    "node-hot-loader": "^1.19.0",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack-cli": "^3.3.11"
  }
}
